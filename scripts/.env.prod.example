# MongoDB credentials
MONGO_INITDB_ROOT_USERNAME=example_user
MONGO_INITDB_ROOT_PASSWORD=example_password
MONGO_URI=mongodb://example_user:example_password@mongodb:27017
MONGO_PORT=27017

# Node.js environment
NODE_ENV=production
SRV_PORT=3000

# Git commit hash for versioning
GIT_COMMIT=latest

# Docker registry credentials - DO NOT COMMIT ACTUAL CREDENTIALS
# Replace these with your actual credentials when running scripts
DOCKER_CREDS_USR=your_docker_username
DOCKER_CREDS_PSW=your_docker_password